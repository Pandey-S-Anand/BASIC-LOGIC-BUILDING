//Using Recursion

class Solution{
  public boolean mixStart(String str) {
    if(str.length()<3){
      return false;
    }
   
    if(str.startsWith("mix") || str.startsWith(str.charAt(0)+"ix")){
      return true;
    }
   
    return mixStart(str.substring(1)); 
  }
}


//Without Recursion

class Solution{
  public boolean mixStart(String str) {
    for(int i=0;i<str.length()-2;i++){
      if(str.startsWith("mix") || str.startsWith(str.charAt(i)+"ix")){
        return true;
      }
    }
   
    return false;
  }
}
