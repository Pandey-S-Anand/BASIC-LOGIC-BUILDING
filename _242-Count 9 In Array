//Using Recursion

class Solution{
  public int arrayCount9(int[] arr) {
    if(arr.length==0){
      return 0;
    }
    return (arr[0]==9?1:0)+arrayCount9(Arrays.copyOfRange(arr, 1, arr.length));
  }
}


//Without Using Recursion

class Solution{
  public int arrayCount9(int[] arr) {
    int count=0;
    for(int i=0;i<arr.length;i++){
       if(arr[i]==9){
          count++;
        }
    }  
    return count;
  }
}
