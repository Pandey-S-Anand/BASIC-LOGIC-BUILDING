class Solution{
    public static char getMaxOccuringChar(String str){
       int[] freqArr=new int[26];
       
       for(char ch:str.toCharArray())
         freqArr[ch-'a']++;
         
       char maxFreqChar='a';
       int maxFreq=freqArr[0];
	   
       for(int i=1;i<26;i++){
          if(freqArr[i]>maxFreq){
              maxFreq=freqArr[i];
              maxFreqChar=(char)(i+'a');
          }
       }
	     
       return maxFreqChar;   
    }
}
