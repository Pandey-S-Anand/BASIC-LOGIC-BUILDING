//Specific Figure Code

public class Solution {
    public static void pattern25(){
       for(int i=1;i<=7;i++)
       {
           for(int j=1;j<=7;j++)
             if((i==1 || i==7 || j==1 || j==7) ||  (i>2 && i<6 && j>2 && j<6 && (i==3 || i==5 || j==3 || j==5)  )
               System.out.print("*");
             else
               System.out.print(" ");
               
            System.out.println();   
       }
    }
}
