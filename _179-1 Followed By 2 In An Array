class Solution {
    public boolean has12(int[] arr) {
        boolean foundTwoOnRight = false;

        for (int i = arr.length - 1; i >= 0; i--) {
            if (arr[i] == 1 && foundTwoOnRight) {
                return true;
            }

            if (arr[i] == 2) {
                foundTwoOnRight = true;
            }
        }

        return false;
    }
}
