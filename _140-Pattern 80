/*Q-To Print

E D C B A 
E D C B  
E D C   
E D    
E 

*/  
  
//Figure Specific Code

class Solution{
  public static void pattern80(){  
      for (int i = 1; i <= 5; i++)
      {
         int data=69;
	 for(int j = 1; j <=5; j++)
	    if(j<=(6-i))
	      System.out.print((char)data-- +" ");
	    else
              System.out.print(" ");
		 
	System.out.println();
      } 
  }
}
