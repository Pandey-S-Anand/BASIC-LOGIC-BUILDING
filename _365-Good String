class Solution {
    String isGoodString(String str) {
        for (int i = 0; i < str.length() - 1; i++) {
            int diff = Math.abs(str.charAt(i) - str.charAt(i + 1));

            if (diff!= 1 && diff!=25) {
                return "NO";
            }
        }

        return "YES";
    }
}

/*
Input: s = "xyzabc"
Output: YES
*/
