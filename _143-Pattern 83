//Figure Specific Code

class Solution{
  public static void pattern83(){  
     int data=1;
     for (int i = 1; i <= 7; i++)
     {
	for(int j = 1; j <=5; j++)
	   if((i==1 || i==4 || i==7) && (j>1 && j<5) || (j==1) && (i==2 || i==3) || (j==5) && (i==5 || i==6))
              System.out.print("*");
	   else
              System.out.print(" ");
		
	System.out.println();
     } 
  }
}
