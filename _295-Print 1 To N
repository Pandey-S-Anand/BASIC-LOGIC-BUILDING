//Using Recursion

class Solution {
    static void printTillN(int n) {
        if (n == 0) {
            return;
        }

        printTillN(n - 1);
        System.out.print(n + " ");
    }
}

// Without Recursion

class Solution {
    static void printTillN(int n) {
        for (int i = 1; i <= n; i++) {
            System.out.print(i + " ");
        }
    }
}
