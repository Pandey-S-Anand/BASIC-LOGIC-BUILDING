/*Q-To Print

55555
45555
34555
23455
12345

*/

//Figure Specific

class Solution{    
    public static void pattern66(){
       for(int i=1;i<=5;i++)
       {
           int data=6-i;
           for(int j=1;j<=5;j++)
             if(j<=i)
               System.out.print(data++);
             else
               System.out.print("5");
               
          System.out.println();   
       }
    }
}
