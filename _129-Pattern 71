/*

AAA AAB AAC ABA ABB ABC ACA ACB ACC 
BAA BAB BAC BBA BBB BBC BCA BCB BCC 
CAA CAB CAC CBA CBB CBC CCA CCB CCC

*/

// Figure based Code

class Solution {
    public static void pattern() {
        for (int row = 65; row <= 67; row++) {
            for (int column = 65; column <= 67; column++) {
                for (int depth = 65; depth <= 67; depth++) {
                    System.out.printf("%c%c%c ", (char) row, (char) column, (char) depth);
                }
            }
            System.out.println();
        }
    }
}
