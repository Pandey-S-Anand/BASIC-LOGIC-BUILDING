class Solution {
    public boolean isPalindrome(int num) {
      if(num<0)
        return false;
        
      if(reverse(num)==num)
         return true;
      else 
         return false;  
    }

    public static int reverse(int num)
    {
	    int rev=0;
	    while(num!=0)
	    {
	     rev=rev*10+num%10;
	     num=num/10;
	    }
      return rev;
    } 
}
